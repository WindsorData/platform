<h1 class="main-title">Insert search results here</h1>
<%= form_tag(file_download_path, class: "main-form" ) do %>
  <% @tickers.each{ |k| %>
    <% if k.is_a? Hash %>
      <%= check_box_tag 'tickers[]', k[:ticker] %> <%= k[:name] %>
      </br>
    <% end %>
  <% } %>
  <button type="submit" class="btn right">Download File</button>
<% end %>