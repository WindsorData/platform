<%= form_tag(send_file_path, multipart: true) do %>
  <%= render 'layouts/messages' %>
  <%= select_tag :type, options_for_select(Constants::FILE_TYPES) %>  
  <%= file_field_tag 'file', required: :true %>
  <%= submit_tag 'Upload', class: 'big-button' %>
  <% if @upload_log %>
    <%= render 'layouts/upload_results', results: @upload_log.results %>
  <% end %>
<% end %>