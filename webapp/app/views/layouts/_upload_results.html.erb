<% results.each do |element| %>
  <%= content_tag :div, "Filename: " + element.file_name, class: "file-name" %>
  <% if element.ticker %>
    <%= content_tag :div, "Ticker: " + element.ticker, class: "ticker-name" %>
  <% end %>
  <ul class="no-bullet">
  	<% if !JSON.parse(element['messages']).empty? %>
	    <% JSON.parse(element['messages']).each do |message| %>
				<%= content_tag :li, :class => "error" do %>
				  <%= message %>
				<% end -%>
	    <% end %>
	  <% else %>
	  	<%= content_tag :li, :class => "success" do %>
			  File successfully parsed.
			<% end -%>
	  <% end %>
  </ul>
<% end %>